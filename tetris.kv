<MainGrid>
    cols: 1
    gameGrid: []
    sizeTile: 10
    clockEvent: None
    on_kv_post: root.setup()
    Label:
        text: "Tetris"
        font_size: 30
        size_hint_y: None
        size_hint: (1, 0.2)
    BoxLayout:
        orientation: 'vertical'
        size_hint_y: None
        size_hint: (1, 0.8)
        padding: 10
    GridLayout:
        cols: 2
        size_hint_y: None
        size_hint: (1, 0.2)
        padding: 10
        Button:
            text: 'Settings'
            on_press: root.settings()
        Button:
            text: "Start"
            on_press: root.start()